{
  "name": "Custom Dnsmasq",
  "version": "1.4.4",
  "slug": "custom-dnsmasq",
  "description": "A simple custom DNS server",
  "url": "https://github.com/rbo/home-assistant-addons/tree/main/dnsmasq",
  "arch": ["aarch64"],
  "advanced": true,
  "startup": "system",
  "init": false,
  "ports": {
    "53/tcp": 53,
    "53/udp": 53
  },
  "options": {
    "defaults": ["8.8.8.8", "8.8.4.4"],
    "forwards": [],
    "hosts": [],
    "services": []
    "wildcards": []
  },
  "schema": {
    "defaults": ["str"],
    "forwards": [
      {
        "domain": "str",
        "server": "str"
      }
    ],
    "hosts": [
      {
        "host": "str",
        "ip": "str"
      }
    ],
    "wildcards": [
      {
        "host": "str",
        "ip": "str"
      }
    ],
    "services": [
      {
        "srv": "str",
        "host": "str",
        "port": "str",
        "priority": "int",
        "weight": "int"
      }
    ]
  },
  "image": "quay.io/rbo/home-assistant-addon-dnsmasq"
}
